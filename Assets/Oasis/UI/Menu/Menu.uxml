            <ui:Label text="Settings" display-tooltip-when-elided="true" name="Settings" style="font-size: 32px; padding-left: 20px; padding-right: 20px; padding-top: 20px; padding-bottom: 20px;" />
            <ui:Label text="Exit" display-tooltip-when-elided="true" name="Exit" style="font-size: 32px; padding-left: 20px; padding-right: 20px; padding-top: 20px; padding-bottom: 20px;" />
        </ui:VisualElement>
    </ui:VisualElement>
</ui:UXML>
